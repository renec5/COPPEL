pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                sh "echo starting execution"
            }
        }
        stage("Test"){
            steps{
                sh "pip3 install pytest"
                sh "python3 -m pytest -s -v /PycharmProjects/COPPEL/Tests/Tests.py --browser chrome"
            }
        }
    }
}
